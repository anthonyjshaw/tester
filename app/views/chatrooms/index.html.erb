<div class="container">
  <%= form_tag my_chatrooms_path, method: :get, class: 'form-inline search-form' do |f| %>
    <%= text_field_tag :username, params[:username], placeholder: "Search a Developer", class: "form-control" %>
    <%= submit_tag "Search", class: "btn btn-primary btn-search pad-small" %>
  <% end %>

  <% @chatrooms.each do |chatroom| %>
    <div class="row">
      <%= link_to chatroom_path(chatroom) do %>
        <%# <%= @user.photo %>
        <%= chatroom.name %>
      <% end %>
    </div>
   <% end %>
</div>


