 <div class="form-container mt-3">
<div class="row justify-content-center ">

<h1>Update your Test</h1>

</div>
<div class="row justify-content-center">
<div class="booking-form form-fields">



<%= simple_form_for(@test) do |f| %>
<div class="row justify-content-center">
<%= f.input :name %>
</div>
<div class="row justify-content-center">
<%= f.input :description %>
</div>
<div class="row justify-content-center">
<%= f.input :test_url %>
</div>
<div class="row justify-content-center">
</div>
<div class="row justify-content-center">
<%= f.submit class: 'btn btn-primary form-btn' %>
</div>
<% end %>
</div>
</div>
</div>
