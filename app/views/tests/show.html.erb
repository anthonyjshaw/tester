
<%= render 'shared/circle_gradient' %>
<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg" style="overflow:visible">
            <defs>
                <linearGradient x1="93.05%" y1="19.767%" x2="15.034%" y2="85.765%" id="header-shape-2">
                    <stop stop-color="#FF3058" offset="0%"></stop>
                    <stop stop-color="#FF6381" offset="100%"></stop>
                </linearGradient>
            </defs>
            <circle class="anime-element fadeup-animation" cx="64" cy="64" r="64" fill="#898ef3" fill-rule="evenodd"></circle>
  </svg>
  <div class="container mt-3">
   <%= render 'tests/components/edit_test_modal' %>
   <%= render 'tests/components/new_review', test: @test, review: @review %>
   <nav aria-label="breadcrumb">
      <ol class="breadcrumb test-breadcrumb">
         <li class="breadcrumb-item "><%= link_to @test.project.name, project_path(@test.project) %></li>
         <li class="breadcrumb-item active" id="chevron" aria-current="page"><%= @test.name %></li>
      </ol>
   </nav>
   <h1 class='project-name'><%= @test.project.name %></h1>
   <%= render 'tests/components/test_header' %>
   <%= render 'tests/components/test_sub_header' %>
   <hr>
   <%= render 'tests/components/reviews' %>
</div>
